   "source": [
    "'''Count the number of even and odd numbers from a series of numbers'''\n",
    "num=(1,2,3,4,5,6,7,8,9)\n",
    "odd=0\n",
    "even=0\n",
    "for i in num:\n",
    "    if not i%2:\n",
    "        even+=1\n",
    "    else:\n",
    "        odd+=1\n",
    "print(\"number of even numbers:\",even)\n",
    "print(\"number of odd numbers:\",odd)"
   ]
  }
